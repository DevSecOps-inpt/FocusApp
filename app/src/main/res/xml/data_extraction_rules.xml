<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="database" path="focuslock_database" />
        <exclude domain="database" path="focuslock_database_debug" />
        <exclude domain="sharedpref" path="focuslock_secure_prefs.xml" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="database" path="focuslock_database" />
        <exclude domain="database" path="focuslock_database_debug" />
        <exclude domain="sharedpref" path="focuslock_secure_prefs.xml" />
    </device-transfer>
</data-extraction-rules> 